# DMI API Configuration
# Copy this file to .env and replace with your actual credentials

# Your DMI Dealer ID
REACT_APP_DMI_DEALER_ID=your_dealer_id_here

# Your DMI REST Code
REACT_APP_DMI_REST_CODE=your_rest_code_here

# Environment: sandbox or production
REACT_APP_DMI_ENVIRONMENT=sandbox

# API Base URLs (usually don't need to change these)
REACT_APP_DMI_PRODUCTION_URL=https://dmidrs.com/dealers/dmirest
REACT_APP_DMI_SANDBOX_URL=https://dmidrs.com/dealers_sb/dmirest 